// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Format {
  name        String   @id
  pattern     String
  description String?
}


model Definition {
  name        String   @id
  format      String
  description String?
}
